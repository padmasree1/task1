pipeline{
 agent any
 stages{
   stage('checkout')
        {
            steps{
            git credentialsId: 'GITcredentials', url: 'https://github.com/padmasree1/task1.git'
            }
        } 
        
        stage('build docker image')
        {
        steps{
        docker build -t apache .
        
        }
        }
    }
 }
