pipeline{
 agent any
 stages{
   stage('checkout'){
        
            steps{
            git branch: 'main', url: 'https://github.com/padmasree1/task1.git'
            }
        } 
        
        stage('build docker image'){
        
        steps{
        sh "docker build -t apache ."
        
        }
        }
    }
 }
